class ListaEncadeada:
    def __init__(self):
        self.inicio = None

    def adicionar_inicio(self, dado):
        novo = No(dado)
        novo.proximo = self.inicio
        self.inicio = novo

    def adicionar_final(self, dado):
        novo = No(dado)
        if self.inicio is None:
            self.inicio = novo
        else:
            atual = self.inicio
            while atual.proximo is not None:
                atual = atual.proximo
            atual.proximo = novo

    def percorrer(self):
        atual = self.inicio
        while atual is not None:
            print(atual.dado)
            atual = atual.proximo
